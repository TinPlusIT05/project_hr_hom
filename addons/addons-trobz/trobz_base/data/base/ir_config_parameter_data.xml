<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="language_date_format" model="ir.config_parameter">
        <field name="key">language_date_format</field>
        <field name="value">%d/%m/%Y</field>
    </record>

    <record id="default_email" model="ir.config_parameter">
        <field name="key">default_email</field>
        <field name="value">noone@trobz.com</field>
    </record>

    <record id="config_base_user_record_rule" model="ir.config_parameter">
        <field name="key">config.base.user.record.rule</field>
        <field name="value">['|', ('company_ids', 'child_of', [user.company_id.id]), ('company_id', '=', user.company_id.id)]</field>
    </record>

    <record id="language_to_load" model="ir.config_parameter">
        <field name="key">language_to_load</field>
        <field name="value">fr_FR</field>
    </record>
</odoo>
